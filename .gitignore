cat > .gitignore <<'EOF'
# env & secrets
.env
.env.*.local

# Python
__pycache__/
*.pyc
*.pyo
*.pyd
.venv/
venv/
.build/

# Editor / OS
.DS_Store
.vscode/
.idea/

# Docker
**/.pytest_cache/
**/.mypy_cache/
**/.coverage
**/htmlcov/
EOF

# agent logs (local only)
.agent_logs/
workspace/.agent_logs/
**/.agent_logs/
*.ndjson
